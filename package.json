{
  "name": "template-server-nodejs-without-sockets",
  "version": "1.0.0",
  "description": "A template for create a new API/REST project in NodeJs with TypeScript. This template doesn't have websockets",
  "main": "./src/index.js",
  "scripts": {
    "start": "cross-env ENV_FILE_PATH='../../.env.prod' npm run build && node dist",
    "start:dev": "cross-env ENV_FILE_PATH='../../.env' nodemon dist",
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:cache": "jest --clearCache"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Platzi-Master-TM-AnaMartinez/template-server-nodejs-without-sockets.git"
  },
  "keywords": [
    "NodeJs",
    "TypeScript"
  ],
  "author": "Bryan Sanchez | @brycatch",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Platzi-Master-TM-AnaMartinez/template-server-nodejs-without-sockets/issues"
  },
  "homepage": "https://github.com/Platzi-Master-TM-AnaMartinez/template-server-nodejs-without-sockets#readme",
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-mongo-sanitize": "^2.0.0",
    "jest": "^26.4.2",
    "mongoose": "^5.10.5"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.8",
    "@types/express-mongo-sanitize": "^1.3.2",
    "@types/mongoose": "^5.7.36",
    "@types/node": "^14.10.1",
    "babel-jest": "^26.3.0",
    "cross-env": "^7.0.2",
    "nodemon": "^2.0.4",
    "ts-jest": "^26.3.0",
    "typescript": "^4.0.2"
  }
}
